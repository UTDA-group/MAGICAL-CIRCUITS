simulator( 'spectre )
design(	 "netlist")
resultsDir( "" )
modelFile( 
    '("toplevel.scs" "top_tt")
)
desVar(	  "vd" 1m	)
desVar(	  "offset" 0	)
temp( 27 ) 
run()
selectResult( 'tran )
